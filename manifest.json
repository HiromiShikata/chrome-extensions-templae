{
    "name": "chrome-extensions-template",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "src/libs/jquery-1.11.1.min.js",
            "src/content.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon" : "icon/icon-64-off.png",
        "default_title": "chome extensions template",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "notifications",
        "http://*/",
        "https://*/"
    ],
    "icons": {
        "128": "icon/icon-64-off.png"
    }
}